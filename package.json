{
  "name": "cloudflare-workers-remix-shopify",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "shopify app dev",
    "dev:remix": "remix watch",
    "start": "wrangler dev --site public --live-reload --test-scheduled",
    "dev:codegen": "npm run codegen -- --watch",
    "deploy": "wrangler deploy --experimental-json-config",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "shopify": "shopify",
    "lint": "eslint . --fix",
    "shopify:config:link": "shopify app config link",
    "shopify:config:push": "shopify app config push",
    "shopify:config:use": "shopify app config use",
    "shopify:generate": "shopify app generate",
    "shopify:deploy": "shopify app deploy",
    "shopify:dev": "remix build --sourcemap && concurrently \"npm:dev:*\"",
    "shopify:env": "shopify app env",
    "db:generate": "drizzle-kit generate:sqlite",
    "db:migrate": "wrangler d1 execute DB --file=./app/utils/db/migrations/0000_certain_outlaw_kid.sql --experimental-json-config",
    "codegen": "graphql-codegen --config .graphqlrc.ts",
    "prepare": "husky install",
    "tunnel": "cloudflared tunnel --config .cloudflared/config.yml --url localhost:8787 run app-template-remix",
    "setup": "./bin/setup.mjs && npm run db:migrate -- --local"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.3.1",
    "@conform-to/react": "^0.9.1",
    "@conform-to/zod": "^0.9.1",
    "@epic-web/cachified": "^4.0.0",
    "@epic-web/invariant": "^1.0.0",
    "@remix-run/cloudflare": "^2.5.1",
    "@remix-run/css-bundle": "^2.5.1",
    "@remix-run/react": "^2.5.1",
    "@shopify/admin-api-client": "^0.2.2",
    "@shopify/app": "^3.55.2",
    "@shopify/cli": "^3.55.2",
    "@shopify/polaris": "^12.11.1",
    "@shopify/react-i18n": "^7.8.0",
    "@shopify/shopify-api": "^9.0.2",
    "@shopify/shopify-app-remix": "^2.4.0",
    "@shopify/shopify-app-session-storage-kv": "^2.0.4",
    "@shopify/storefront-api-client": "^0.2.2",
    "@web3-storage/multipart-parser": "^1.0.0",
    "cachified-adapter-cloudflare-kv": "^1.0.1",
    "drizzle-orm": "^0.29.3",
    "graphql-request": "^6.1.0",
    "isbot": "^4.4.0",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@clack/prompts": "^0.7.0",
    "@cloudflare/workers-types": "^4.20240117.0",
    "@parcel/watcher": "^2.4.0",
    "@remix-run/dev": "^2.5.1",
    "@remix-run/eslint-config": "^2.5.1",
    "@shopify/api-codegen-preset": "^0.0.1",
    "@shopify/app-bridge-types": "^0.0.5",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/node": "^20.11.10",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.7",
    "concurrently": "^8.2.2",
    "drizzle-kit": "^0.20.13",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.7",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.4",
    "prettier-plugin-sql": "^0.18.0",
    "typescript": "^5.3.3",
    "wrangler": "3.26.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "workspaces": {
    "packages": [
      "extensions/*"
    ]
  },
  "trustedDependencies": [
    "@shopify/plugin-cloudflare"
  ],
  "standard": {
    "ignore": [
      "node_modules",
      "build",
      "public/build",
      "shopify-app-remix",
      "*/*.yml",
      ".shopify",
      "app/generated"
    ]
  }
}
