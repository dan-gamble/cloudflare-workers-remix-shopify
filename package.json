{
  "name": "cloudflare-workers-remix-shopify",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "shopify app dev --no-update",
    "build": "remix build",
    "deploy": "wrangler deploy",
    "cf:dev": "remix dev --manual -c \"npm start\"",
    "start": "wrangler dev ./build/index.js",
    "typecheck": "tsc",
    "shopify": "shopify",
    "shopify:config:link": "shopify app config link",
    "shopify:config:push": "shopify app config push",
    "shopify:config:use": "shopify app config use",
    "shopify:generate": "shopify app generate",
    "shopify:deploy": "shopify app deploy",
    "shopify:env": "shopify app env",
    "db:generate": "drizzle-kit generate:sqlite",
    "db:up": "drizzle-kit up:sqlite --schema=app/utils/db/schema.server.ts"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.1.3",
    "@conform-to/react": "^0.9.1",
    "@conform-to/zod": "^0.9.1",
    "@epic-web/cachified": "^4.0.0",
    "@remix-run/cloudflare": "^2.3.1",
    "@remix-run/css-bundle": "^2.3.1",
    "@remix-run/react": "^2.3.1",
    "@shopify/app": "^3.51.2",
    "@shopify/cli": "^3.51.2",
    "@shopify/polaris": "^12.1.1",
    "@shopify/shopify-api": "^8.1.1",
    "@shopify/shopify-app-remix": "^2.1.0",
    "@shopify/shopify-app-session-storage-kv": "^2.0.1",
    "cachified-adapter-cloudflare-kv": "^1.0.1",
    "drizzle-orm": "^0.29.1",
    "isbot": "^3.6.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230518.0",
    "@remix-run/dev": "^2.3.1",
    "@remix-run/eslint-config": "^2.3.1",
    "@shopify/app-bridge-types": "^0.0.3",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "drizzle-kit": "^0.20.6",
    "eslint": "^8.38.0",
    "prettier": "^3.1.0",
    "typescript": "^5.1.6",
    "wrangler": "3.8.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "workspaces": {
    "packages": [
      "extensions/*"
    ]
  },
  "trustedDependencies": [
    "@shopify/plugin-cloudflare"
  ]
}
